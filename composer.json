{
  "name": "premierstacks/laravel-template",
  "description": "laravel template",
  "type": "project",
  "keywords": [
    "premierstacks",
    "laravel-template",
    "tomchochola"
  ],
  "homepage": "https://github.com/premierstacks/laravel-template",
  "readme": "./README.md",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tomáš Chochola",
      "email": "chocholatom1997@gmail.com",
      "homepage": "https://github.com/tomchochola",
      "role": "Author"
    }
  ],
  "support": {
    "email": "chocholatom1997@gmail.com",
    "issues": "https://github.com/premierstacks/laravel-template/issues",
    "source": "https://github.com/premierstacks/laravel-template.git",
    "docs": "https://github.com/premierstacks/laravel-template"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/tomchochola"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "laravel/tinker": "^2.9",
    "premierstacks/laravel-stack": "@dev"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0",
    "spatie/laravel-ignition": "^2.4",
    "premierstacks/php-cs-fixer-stack": "@dev",
    "premierstacks/phpstan-stack": "@dev"
  },
  "autoload": {
    "psr-4": {
      "App\\": "./app",
      "Database\\Factories\\": "./database/factories",
      "Database\\Seeders\\": "./database/seeders"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/premierstacks/php-cs-fixer-stack.git"
    },
    {
      "type": "git",
      "url": "https://github.com/premierstacks/phpstan-stack.git"
    },
    {
      "type": "git",
      "url": "https://github.com/premierstacks/php-stack.git"
    },
    {
      "type": "git",
      "url": "https://github.com/premierstacks/laravel-stack.git"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    },
    "preferred-install": "dist",
    "audit": {
      "abandoned": "fail"
    },
    "optimize-autoloader": true,
    "platform-check": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}
